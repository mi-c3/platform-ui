{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": 2,
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "components": "./src/components",
          "styles": "./src/styles",
          "utils": "./src/utils"
        }
    }]
  ]
}
